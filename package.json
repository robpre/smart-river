{
  "name": "smartriver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "build:ci": "next build",
    "postbuild:ci": "dotenv -e .env.deploy -- yarn deploy:ci && mv .secrets.json .next/",
    "start": "next start",
    "lint": "next lint",
    "synth:local": "APP_STAGE=local cdk synth",
    "deploy:local": "APP_STAGE=local cdk deploy 2>&1 | ts-node ops/filter.ts",
    "deploy:ci": "cdk deploy --require-approval never 2>&1 | ts-node ops/filter.ts",
    "parse": "for f in manual-scraped-data/*.csv; do csvtojson \"${f}\" > \"${f}.json\"; done"
  },
  "lint-staged": {
    "*.{ts,js,tsx,json,md}": "prettier --write"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.188.0",
    "@aws-sdk/client-s3": "3.188.0",
    "@aws-sdk/client-ses": "3.188.0",
    "@aws-sdk/lib-dynamodb": "3.188.0",
    "@aws-sdk/types": "3.188.0",
    "next": "12.3.1",
    "node-fetch": "3.2.10",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/jest": "29.1.1",
    "@types/node": "16.11.63",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/webpack": "5.28.0",
    "aws-cdk": "2.44.0",
    "aws-cdk-lib": "2.44.0",
    "cdk": "2.44.0",
    "constructs": "10.1.119",
    "csvtojson": "2.0.10",
    "dotenv-cli": "6.0.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0"
  }
}
